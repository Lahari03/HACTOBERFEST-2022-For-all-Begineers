// https://codeforces.com/problemset/problem/1288/B

#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int T;
    cin >> T;
    while (T--)
    {
        long long int A,B;
        cin >> A >> B;
        B=B+1;
        string str = to_string(B);
        cout << A*(str.length()-1) << endl;
    }
    return 0;
}
